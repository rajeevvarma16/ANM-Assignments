#!/usr/bin/perl



use Net::SNMP;
use DBD::mysql;
use DBI;
use Net::SNMP;
require "db.conf";


#connect to the database
$dbh= DBI->connect("DBI:mysql:database=$database;host=$host",  $username, $password);
$id = <>;
$name = <>;
#create table
 $dbh->do("CREATE TABLE examples (id INTEGER, name VARCHAR(20))");
$dbh->do("INSERT INTO examples VALUES (?, ?)", undef,$id,$name);
